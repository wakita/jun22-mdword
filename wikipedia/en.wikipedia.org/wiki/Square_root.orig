        <mi>j</mi>
        <mo>+</mo>
        <mi>c</mi>
        <mi>k</mi>
        <mo>&#x2223;<!-- ∣ --></mo>
        <msup>
          <mi>a</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msup>
        <mo>+</mo>
        <msup>
          <mi>b</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msup>
        <mo>+</mo>
